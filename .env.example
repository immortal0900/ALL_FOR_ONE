# RAG Commander - Environment Variables Template
# Copy this file to .env and fill in your actual API keys

# Database (Required)
POSTGRES_URL=postgresql://postgres:postgres@localhost:5432/ragdb

# FastAPI URL (for Streamlit to connect to backend)
# Local: http://localhost:8080
# Railway: Will be set automatically by start.sh
FASTAPI_URL=http://localhost:8080

# LLM Services (At least one required - OpenAI recommended)
OPENAI_API_KEY=sk-proj-your-openai-api-key-here
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key-here
GEMINI_API_KEY=your-gemini-api-key-here
COHERE_API_KEY=your-cohere-api-key-here

# Search & Web Scraping (Recommended for report generation)
TAVILY_API_KEY=tvly-your-tavily-api-key-here
PERPLEXITY_API_KEY=pplx-your-perplexity-api-key-here
FIRECRAWL_API_KEY=fc-your-firecrawl-api-key-here

# Korean Real Estate APIs (Required for data retrieval)
R_ONE_API_KEY=your-r-one-api-key-here
MOLIT_API_KEY=your-molit-api-key-here
REAL_TIME_SALE_SEARCH_API_KEY=your-real-time-sale-api-key-here
GONG_GONG_DATA_API_KEY=your-gong-gong-data-api-key-here
KAKAO_REST_API_KEY=your-kakao-rest-api-key-here

# Statistics APIs (Optional)
KOSIS_CONSUMER_KEY=your-kosis-consumer-key-here
KOSIS_CONSUMER_SECRET_KEY=your-kosis-consumer-secret-key-here
ECOS_API_KEY=your-ecos-api-key-here
FRED_API_KEY=your-fred-api-key-here

# MCP Key (Optional - for specific integrations)
MCP_KEY=your-mcp-key-here

# LangSmith Tracing (Optional - for debugging)
LANGSMITH_API_KEY=lsv2_your-langsmith-api-key-here
LANGSMITH_TRACING=false

# Notes:
# - API keys without % encoding are recommended for MOLIT and GONG_GONG_DATA
# - Set LANGSMITH_TRACING=true only when debugging (increases costs)
# - For Railway deployment, set these in the Railway dashboard
